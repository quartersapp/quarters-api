{
  "name": "quarters-api",
  "version": "0.0.1",
  "description": "GraphQL API Server for PS",
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:. node bin/server.js | bunyan",
    "console": "NODE_PATH=$NODE_PATH:. node",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "migrate-test": "NODE_ENV=test knex migrate:latest",
    "rollback-test": "NODE_ENV=test knex migrate:rollback",
    "migrate-all": "npm run migrate && npm run migrate-test",
    "rollback-all": "npm run rollback && npm run rollback-test",
    "test": "NODE_ENV=test jest --config jest-config.json --runInBand",
    "posttest": "standard"
  },
  "author": "Thomas Dashney",
  "license": "ISC",
  "devDependencies": {
    "jest": "^20.0.4",
    "moment": "^2.18.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "apollo-server-koa": "^1.1.0",
    "bcrypt": "^1.0.3",
    "bunyan": "^1.8.12",
    "config": "^1.26.2",
    "graphql": "^0.10.5",
    "graphql-tools": "^1.2.1",
    "jsonwebtoken": "^7.4.3",
    "knex": "^0.13.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4",
    "objection": "^0.8.8",
    "pg": "^7.2.0",
    "serverless-http": "^1.5.0"
  }
}

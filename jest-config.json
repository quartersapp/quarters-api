{
  "modulePaths": ["<rootDir>"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/test-helpers",
    "<rootDir>/node_modules/"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 97
    },
    "./lib/admin-server/": {
      "lines": 0
    }
  },
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!koa-bodyparser|koa-mount).*/.*"
  ],
  "testEnvironment": "node"
}
